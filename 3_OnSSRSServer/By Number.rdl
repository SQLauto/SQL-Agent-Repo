        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>12500f6b-9c3e-4bbd-a0e5-07141fee745d</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="GridData">
      <Query>
        <DataSourceName>localhost</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@myStartDate">
            <Value>=Parameters!prmStartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@myEndDate">
            <Value>=Parameters!prmEndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@myServers">
            <Value>=Parameters!prmServers.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
     ErrorNumber,
     count(*) as 'Count'
FROM 
     [SQLAlerts].[dbo].[AgentAlerts] 
where 
     EventTime between @myStartDate and @myEndDate and
     Server in (@myServers)
GROUP BY
     ErrorNumber
order by 
     1,2</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ErrorNumber">
          <DataField>ErrorNumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Count">
          <DataField>Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Servers">
      <Query>
        <DataSourceName>localhost</DataSourceName>
        <CommandText>SELECT 
	distinct
    [Server]
      
FROM
	 [SQLAlerts].[dbo].[AgentAlerts]
order by
	1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Server">
          <DataField>Server</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
